package Unit1;

public class SequenceStructure {
    public static void main(String[]args){
        System.out.println("Hello World.");
        
        int num1 = 10;
        int num2 = 15;
        
        int sum = num1 + num2;
        int product = num1 * num2;
        
        System.out.println("The sum is " + sum);
        System.out.println("The product is " + product);
    }
}
